@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

if(NOT @BUILD_SHARED_LIBS@)
    find_dependency(Qt@QT_MAJOR_VERSION@Core)
    find_dependency(Qt@QT_MAJOR_VERSION@Gui)
    find_dependency(Qt@QT_MAJOR_VERSION@Qml)
    if (@Qt5X11Extras_FOUND@)
        find_dependency(Qt5X11Extras)
    endif()

    find_dependency(FFmpeg)
    find_dependency(Libmpv)
endif()

find_dependency(Qt@QT_MAJOR_VERSION@Quick)
list(PREPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")
find_dependency(Libmpv)

include("${CMAKE_CURRENT_LIST_DIR}/MpvQtTargets.cmake")
@PACKAGE_INCLUDE_QCHTARGETS@
